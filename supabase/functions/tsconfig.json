{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "lib": ["dom", "esnext"],
    "types": ["./node_modules/deno-types"],
    "allowJs": true,
